# Ignore compiled binaries
*.exe
*.dll
*.so
*.dylib

# Ignore dependencies and package files
/node_modules
/package-lock.json
/yarn.lock

# Ignore build output
/build
/dist

# Ignore IDE and editor files
/.vscode
/.idea
*.code-workspace
*.sublime-project
*.sublime-workspace

# Ignore logs and temporary files
/logs
/tmp
*.log
*.tmp

# Ignore sensitive or environment-specific files
/.env
/.env.local
/.env.*.local
.env.development.local
.env.test.local
.env.production.local

# Ignore OS and editor-specific files
.DS_Store
Thumbs.db
*.swp
*.swo
*.bak

# Ignore user-specific files
/.history
/.bash_history
/.viminfo
/.gitconfig.local
